{
	"name": "unknownbliss/zurb-foundation-bundle",
	"description": "A quick symfony bundle including the zurb foundation to allow easy updating of the foundation across symfony sites.",
	"keywords": ["zurb", "foundation", "bundle", "symfony"],
	"type": "symfony-bundle",
	"homepage": "http://michaelcullum.com",
	"license": "MIT",
	"authors": [
		{
			"name": "MichaelC",
			"username": "unknownbliss",
			"email": "m@michaelcullum.com",
			"homepage": "https://michaelcullum.com/"
		}
	],
    "support": {
        "irc": "irc://irc.freenode.org/unknownbliss"
    },
	"require": {
		"symfony/symfony": "2.1.*"
	},
	"minimum-stability": "dev",
	"autoload": {
		"psr-0": { "Unknownbliss\\ZurbFoundationBundle": "" }
	},
	"target-dir": "Unknownbliss/ZurbFoundationBundle",
	"extra": {
		"branch-alias": {
			"dev-master": "3.2.*-dev"
		}
	}
}
